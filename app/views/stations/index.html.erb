<script src="https://polyfill.io/v3/polyfill.min.js?features=default"></script>

<h2>Find Valetbike Near You! </h2>
<br />
<div style="height:500px; width:100%;">
<div id="map"></div>
</div>
<script
  src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAdq5qVkBOqdKzjnsaBs2hcbyJGrfOFwWc&callback=initMap&v=weekly"
  async
></script>
<br />
<br />
<h2>Stations! </h2>
<br />
<div class='row justify-content-center gy-5'>
<% if @stations.present? %>
  <%= render(partial: "stations/row", collection: @stations, as: :station) %>
<% else %>
</div>
<div class="empty">
  No stations found.
</div>
<% end %>
<div class="my-10">
</div>

